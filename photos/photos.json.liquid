{% for page in site.pages %}{% if page.title == 'Photos' %}
[
{% for post in page.posts offset:3 %}{
    "title": "{{ post.title }}",
    "date": "{{ post.published_at }}",
    "human_date": "{{post.date | date: "%b %d, %Y" }}",
    "url": "{{ post.assets.first.url }}"
}{% if post != page.posts.last %},{% endif %}
{% endfor %}]
{% endif %}{% endfor %}
